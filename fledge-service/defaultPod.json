{
    "metadata":{
       "name":"%podname%",
       "namespace":"default",
       "creationTimestamp":null
    },
    "spec":{
        "selector": {
            "matchLabels": {
                "app":"%podname%"
            }
        },
       "replicas":1,
       "template":{
          "metadata":{
             "creationTimestamp":null,
             "labels":{
                "app":"%podname%"
             }
          },
          "spec":{
             "volumes":[
                {
                   "name":"kubeconfig",
                   "hostPath":{
                      "path":"/var/run/secrets/kubernetes.io/serviceaccount",
                      "type":"Directory"
                   }
                }
             ],
             "containers":[
                {
                   "name":"%podname%",
                   "image":"togoetha/virtual-kubelet",
                   "command":[
                      "/usr/bin/virtual-kubelet"
                   ],
                   "args":[
                      "--provider",
                      "web",
                      "--nodename",
                      "%podname%",
                      "--log-level",
                      "debug"
                   ],
                   "env":[
                      {
                         "name":"WEB_ENDPOINT_URL",
                         "value":"%serviceUrl%"
                      },
                      {
                         "name":"KUBERNETES_SERVICE_HOST",
                         "value":"%kubernetesHost%"
                      },
                      {
                         "name":"KUBERNETES_SERVICE_PORT",
                         "value":"%kubernetesPort%"
                      },
                      {
                         "name":"KUBELET_PORT",
                         "value":"%kubeletPort%"
                      }
                   ],
                   "resources":{
 
                   },
                   "volumeMounts":[
                      {
                         "name":"kubeconfig",
                         "mountPath":"/var/run/secrets/kubernetes.io/serviceaccount"
                      }
                   ]
                }
             ],
             "nodeSelector":{
                "vpodmaster":"true"
             }
          }
       },
       "strategy":{
 
       }
    },
    "status":{
 
    }
 }